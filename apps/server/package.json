    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "cross-env": "^7.0.3",
    "esno": "^4.0.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4"
  }
