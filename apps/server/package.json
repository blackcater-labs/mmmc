  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "build": "nest build",
    "start": "cross-env NODE_ENV=production node dist/main",
    "clean": "rimraf dist",
    "prisma:reset": "npm run prisma:migrate:reset && prisma db push && npm run prisma:client && npm run prisma:seed:dev",
    "prisma:seed": "esno prisma/seed.ts",
    "prisma:seed:dev": "esno prisma/seed.dev.ts",
    "prisma:client": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:migrate:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
