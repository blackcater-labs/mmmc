{
  "name": "@mmmc/lens",
  "type": "module",
  "version": "0.0.1",
  "description": "Graphql Lens for mmmc",
  "author": {
    "name": "blackcater <Elon Tang>",
    "email": "i@blackcater.dev",
    "url": "https://www.blackcater.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/blackcater-labs/mmmc/tree/main/packages/theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackcater-labs/mmmc.git",
    "directory": "packages/theme"
  },
  "bugs": {
    "url": "https://github.com/blackcater-labs/mmmc/issues"
  },
  "keywords": [
    "graphql"
  ],
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
