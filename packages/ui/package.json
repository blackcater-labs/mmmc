{
  "name": "@mmmc/ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "TailwindCSS Plugin for UI theme",
  "author": {
    "name": "blackcater <Elon Tang>",
    "email": "i@blackcater.dev",
    "url": "https://www.blackcater.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/blackcater-labs/mmmc/tree/main/packages/theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackcater-labs/mmmc.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/blackcater-labs/mmmc/issues"
  },
  "keywords": [
    "tailwindcss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mmmc/theme": "workspace:*",
    "@nextui-org/react": "^2.2.9",
    "@nextui-org/theme": "^2.1.17",
    "@tabler/icons-react": "^2.47.0",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.3",
    "react-day-picker": "^8.10.0"
  },
  "devDependencies": {
    "@mmmc/test-utils": "workspace:*",
    "@testing-library/jest-dom": "^6.4.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "vitest": "^1.2.2"
  }
}
