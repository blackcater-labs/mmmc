{
  "name": "@mmmc/theme",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "TailwindCSS Plugin for UI theme",
  "author": {
    "name": "blackcater <Elon Tang>",
    "email": "i@blackcater.dev",
    "url": "https://www.blackcater.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/blackcater-labs/mmmc/tree/main/packages/theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackcater-labs/mmmc.git",
    "directory": "packages/theme"
  },
  "bugs": {
    "url": "https://github.com/blackcater-labs/mmmc/issues"
  },
  "keywords": [
    "tailwindcss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nextui-org/theme": "^2.1.17",
    "clsx": "^2.1.0",
    "deepmerge": "^4.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1"
  }
}
